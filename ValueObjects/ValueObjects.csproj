    <PackageProjectUrl>https://github.com/kchopein/ValueObjects</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kchopein/ValueObjects</RepositoryUrl>
    <Authors>KchoPein!</Authors>
    <Company>KchoPein!</Company>
